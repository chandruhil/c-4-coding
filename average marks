
import java.util.Scanner;

	public class Solution{
    public static void main(String args[]) {
        
    	 
		Scanner s = new Scanner(System.in);
        char ch = s.next().charAt(0);
            
      int a ,b,c,ave;
                a = s.nextInt();
                b = s.nextInt();
                c = s.nextInt();
                ave = (a+b+c)/3;
             System.out.println(ch);
             System.out.println(ave);      
    }
}
